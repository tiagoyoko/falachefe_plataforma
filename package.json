{
  "name": "agentic-coding-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "tsx scripts/build-check.ts && next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:dev": "drizzle-kit push",
    "db:reset": "drizzle-kit drop && drizzle-kit push",
    "db:test": "tsx scripts/test-db-connection.ts",
    "db:setup": "tsx scripts/setup-database.ts",
    "db:migrate-manual": "tsx scripts/migrate-database.ts",
    "db:migrate-robust": "tsx scripts/migrate-database-robust.ts",
    "db:clean": "tsx scripts/clean-database.ts",
    "db:remove-old": "tsx scripts/remove-old-tables.ts",
    "db:create-missing": "tsx scripts/create-missing-tables.ts",
    "db:create-billing": "tsx scripts/create-billing-tables.ts",
    "admin:set-user": "tsx scripts/set-admin-user.ts",
    "admin:list": "tsx scripts/list-admin-users.ts",
    "admin:test": "tsx scripts/test-admin-permissions.ts",
    "db:verify": "tsx scripts/verify-table-structure.ts",
    "db:fix-structure": "tsx scripts/fix-table-structure.ts",
    "db:fix-structure-v2": "tsx scripts/fix-table-structure-v2.ts",
    "auth:test": "tsx scripts/test-auth-system.ts",
    "auth:config": "tsx scripts/test-auth-config.ts",
    "auth:better-test": "tsx scripts/test-better-auth-config.ts",
    "email:test": "tsx scripts/test-email-service.ts",
    "validate:uaz": "tsx scripts/validate-uaz-integration.ts",
    "test:uaz": "jest src/lib/uaz-api/__tests__",
    "test:uaz:watch": "jest src/lib/uaz-api/__tests__ --watch",
    "test:uaz:coverage": "jest src/lib/uaz-api/__tests__ --coverage",
    "test:agents": "node scripts/test-agents.js",
    "test:agents:unit": "npx jest --config jest.config.agents.js --testPathPattern='.*\\.test\\.ts$' --testPathIgnorePatterns='.*\\.performance\\.test\\.ts$'",
    "test:agents:performance": "npx jest --config jest.config.agents.js --testPathPattern='.*\\.performance\\.test\\.ts$'",
    "test:agents:integration": "npx jest --config jest.config.agents.js --testPathPattern='.*integration.*\\.test\\.ts$'",
    "test:agents:coverage": "npx jest --config jest.config.agents.js --coverage",
    "test:agents:watch": "npx jest --config jest.config.agents.js --watch",
    "validate:agent-squad": "node scripts/validate-agent-squad.js",
    "bmad:refresh": "bmad-method install -f -i codex",
    "bmad:list": "bmad-method list:agents",
    "bmad:validate": "bmad-method validate"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.9",
    "@ai-sdk/react": "^2.0.9",
    "@openai/agents": "^0.1.9",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-email/render": "^1.3.1",
    "@tanstack/react-query": "^5.90.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/redis": "^4.0.10",
    "@types/uuid": "^10.0.0",
    "@upstash/redis": "^1.35.4",
    "ai": "^5.0.9",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.4",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "next-themes": "^0.4.6",
    "openai": "^6.1.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.2.1",
    "redis": "^5.8.3",
    "resend": "^6.1.2",
    "sonner": "^2.0.7",
    "stripe": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.19",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jest": "^29.7.0",
    "shadcn": "^3.0.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
